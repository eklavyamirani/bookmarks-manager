{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ports": {
    "http": 5000,
    "https": 5001
  },
  "ConnectionStrings": {
    "PostgresConnection": "Host=localhost;Database={PostgresDb};Username={PostgresUser};Password={PostgresPassword}"
  },
  "UsePostgres": true
}
